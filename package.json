{
  "name": "webhive",
  "version": "0.0.0-dev",
  "license": "MIT",
  "description": "",
  "main": "index.js",
  "typings": "index.d.ts",
  "author": "2019",
  "engines": {
    "node": ">=8.3",
    "npm": ">=3"
  },
  "scripts": {
    "dev": "run-p dev:api dev:web",
    "dev:api": "ts-node-dev --no-deps --transpileOnly -- src/webhive.api/main.ts",
    "dev:api:d": "ts-node-dev --no-deps --transpileOnly --inspect -- src/webhive.api/main.ts",
    "dev:web": "ts-node-dev --no-deps --transpileOnly -- src/webhive.web/main.ts",
    "test": "npm run eslint && npm run tscheck && npm run test:cov",
    "test:r": "node node_modules/mocha/bin/_mocha --node_env=test -r ts-node/register/transpile-only -r @abraham/reflection --timeout 8s src/**/*.spec.ts",
    "test:w": "node node_modules/mocha/bin/_mocha --node_env=test -r ts-node/register/transpile-only -r @abraham/reflection --timeout 8s --watch-extensions ts --watch src/**/*.spec.ts",
    "test:cov": "nyc mocha src/**/*.spec.ts",
    "test:d": "node --inspect-brk -r ts-node/register/transpile-only -r @abraham/reflection node_modules/mocha/bin/_mocha --no-timeouts --watch-extensions ts --watch src/**/*.spec.ts",
    "tscheck": "echo tscheck... && tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "tsclint": "tsc --noEmit --strict --forceConsistentCasingInFileNames --noImplicitReturns --noImplicitThis --noUnusedLocals --noUnusedParameters",
    "tsclint:w": "npm run tsclint -- --watch",
    "eslint:w": "sh Taskfile eslint_watch",
    "eslint": "eslint src --ext ts",
    "eslint:fix": "eslint src --ext ts --fix",
    "tslint:fix": "tslint -p tsconfig.json --fix",
    "lint:w": "run-p tsclint:w eslint:w",
    "prepublishOnly": "sed -i -e 's/devDependencies/_devDependencies/g' package.json",
    "copySource": "cp -rv src dist && find dist/src -name '*.spec.ts' | xargs rm -f",
    "build": "tsc",
    "commit": "git-cz",
    "rebuild_lock": "rm package-lock.json && npm prune && npm install",
    "typesync": "npx typesync"
  },
  "dependencies": {
    "@abraham/reflection": "0.5.1",
    "await-to-js": "2.1.1",
    "class-transformer": "0.2.0",
    "class-validator": "0.9.1",
    "convict": "4.4.1",
    "feedparser": "2.2.9",
    "got": "9.6.0",
    "koa": "2.7.0",
    "koa-bodyparser": "4.2.1",
    "koa-favicon": "2.0.1",
    "koa-json-error": "3.1.2",
    "koa-static": "5.0.0",
    "koa-tree-router": "0.4.5",
    "loud-rejection": "1.6.0",
    "mongodb": "3.1.13",
    "ms": "2.1.1",
    "njct": "7.1.3",
    "tinydate": "1.0.1",
    "ucfirst": "1.0.0",
    "vhtml": "2.1.0",
    "yargs": "13.2.2"
  },
  "devDependencies": {
    "@types/convict": "^4.2.1",
    "@types/eslint": "^4.16.6",
    "@types/expect": "1.X",
    "@types/feedparser": "^2.2.2",
    "@types/got": "^9.4.1",
    "@types/html-entities": "^1.2.16",
    "@types/into-stream": "^3.1.0",
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "4.2.2",
    "@types/koa-favicon": "^2.0.19",
    "@types/koa-json-error": "^3.1.2",
    "@types/koa-static": "^4.0.1",
    "@types/loud-rejection": "^1.6.0",
    "@types/mocha": "^5.2.6",
    "@types/mongodb": "^3.1.20",
    "@types/mongodb-memory-server": "1.8.0",
    "@types/ms": "0.7.30",
    "@types/node": "^11.10.4",
    "@types/yargs": "12.0.9",
    "@typescript-eslint/eslint-plugin-tslint": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "eslint": "^5.15.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-tslint": "^3.1.0",
    "eslint-plugin-unicorn": "^7.1.0",
    "expect": "1.X",
    "html-entities": "^1.2.1",
    "inotifywait-win32": "^1.0.0",
    "into-stream": "^4.0.0",
    "koa-preuse": "0.0.0",
    "koa-sham": "0.0.5",
    "mocha": "^6.0.2",
    "mongodb-memory-server": "3.1.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.3.0",
    "ts-node": "^8.0.2",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.13.1",
    "tslint-clean-code": "^0.2.9",
    "tslint-microsoft-contrib": "^6.0.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "~3.3.3333"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "heroku-run-build-script": true
}
